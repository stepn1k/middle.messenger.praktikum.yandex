{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2019",
    "declaration": false,
    "module": "commonjs",
    "strictNullChecks": false,
    "sourceMap": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true
  },
  "exclude": [
    "node_modules",
    "**/*.spec.ts"
  ]
}
